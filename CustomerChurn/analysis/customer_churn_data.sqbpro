w_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="Total Customers and Churn Rate*">SELECT 
    COUNT(*) AS Total_Customers,
    SUM(CASE WHEN [Churn Label]='Yes' THEN 1 ELSE 0 END) AS Churned_Customers,
    ROUND(SUM(CASE WHEN [Churn Label]='Yes' THEN 1 ELSE 0 END)*100.0/COUNT(*),2) AS Churn_Rate
FROM churn_data_cleaned;</sql><sql name="Churn by Contract Type*">SELECT Contract,
    COUNT(*) AS Total_Customers,
    SUM(CASE WHEN [Churn Label]='Yes' THEN 1 ELSE 0 END) AS Churned,
    ROUND(SUM(CASE WHEN [Churn Label]='Yes' THEN 1 ELSE 0 END)*100.0/COUNT(*),2) AS Churn_Rate
FROM churn_data_cleaned
GROUP BY Contract
ORDER BY Churn_Rate DESC;</sql><sql name="Churn by Payment Method*">SELECT [Payment Method],
    ROUND(SUM(CASE WHEN [Churn Label]='Yes' THEN 1 ELSE 0 END)*100.0/COUNT(*),2) AS Churn_Rate
FROM churn_data_cleaned
GROUP BY [Payment Method];</sql><sql name="Churn by Internet Service*">SELECT [Internet Service],
    ROUND(SUM(CASE WHEN [Churn Label]='Yes' THEN 1 ELSE 0 END)*100.0/COUNT(*),2) AS Churn_Rate
FROM churn_data_cleaned
GROUP BY [Internet Service];</sql><sql name="Average Tenure and Charges by Churn*">SELECT [Churn Label],
    ROUND(AVG([Tenure Months]),1) AS Avg_Tenure,
    ROUND(AVG([Monthly Charges]),2) AS Avg_Monthly_Charge
FROM churn_data_cleaned
GROUP BY [Churn Label];</sql><current_tab id="4"/></tab_sql></sqlb_project>
